{
  "name": "tails-n-trades-backend-server",
  "version": "1.0.50",
  "scripts": {
    "migrate": "npx @better-auth/cli migrate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "db:generate": "drizzle-kit generate --config=./src/configs/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./src/configs/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./src/configs/drizzle.config.ts",
    "db:docker:migrate": "docker exec -it tails-n-trades-backend-server bun run db:migrate",
    "db:docker:studio": "docker exec -it tails-n-trades-backend-server bun run db:studio --host 0.0.0.0",
    "docker:start": "bun scripts/ops.ts start",
    "docker:stop": "bun scripts/ops.ts stop",
    "docker:reset": "bun scripts/ops.ts reset",
    "docker:restart": "bun scripts/ops.ts restart",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@types/amqplib": "^0.10.8",
    "@types/pg": "^8.16.0",
    "amqplib": "^0.10.9",
    "better-auth": "^1.4.6",
    "drizzle-kit": "^x.x.x",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.18",
    "ioredis": "^5.8.2",
    "kysely": "^0.28.8",
    "pg": "^8.16.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "bun-types": "latest"
  },
  "module": "src/index.js",
  "type": "module",
  "private": true,
  "peerDependencies": {
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
